<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Trang chá»§</title>
    <link rel="stylesheet" href="/css/styles.css" />
  </head>
  <body>
    <div class="topnav">
      <a class="active" href="#home">Home</a>
      <a href="#news">News</a>
      <a href="#contact">Contact</a>
      <a href="#about">About</a>
    </div>

    <div style="padding: 16px">
      <h1>User list</h1>
      <table id="customers">
        <thead>
          <tr>
            <th>ID</th>
            <th>Email</th>
            <th>Username</th>
            <th>Address</th>
            <th>Action</th>
          </tr>
        </thead>
        <% listUser.forEach((user, index) => { %>
        <tbody>
          <tr>
            <td><%=user.id %></td>
            <td><%=user.email %></td>
            <td><%=user.username %></td>
            <td><%=user.address %></td>
            <td>
              <a href="user/detail/<%=user.id %>">Details</a>
              <a href="user/edit/<%=user.id %>">Edit</a>
              <a href="user/delete/<%=user.id %>">Delete</a>
            </td>
          </tr>
        </tbody>

        <% }) %>
      </table>
    </div>
  </body>
</html>
